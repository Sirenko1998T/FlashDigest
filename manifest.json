{
   "manifest_version": 3,
   "name": "Flash Digest",
   "description": "Base Level Extension",
   "version": "1.1",
   "permissions": [
     "activeTab",
     "scripting",
     "storage"
   ],
   "action": {
     "default_popup": "popup/popup.html",
     "default_icon": "icon/f.png"
   },
   "content_scripts": [
     {
       "matches": ["<all_urls>"],
       "js": ["content.js"]
     }
   ]
}
